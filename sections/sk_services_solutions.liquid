<section id="sk_services" class="sk_solutions">
  <a href="{{ settings.promo_btn_url }}">
    <div class="sk_service_promo_box mobile_only">
      <p class="sk_promo_title"><span class="current_month"></span>{{ settings.promo_title }}</p>
      <p class="info">{{ settings.promo_sub_title }}</p>
